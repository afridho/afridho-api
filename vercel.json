{
    "version" : 2,
    "builds":[
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "client/build/**",
            "use": "@vercel/static"
        },
        {
            "src": "frontend/sqlite/**",
            "use": "@vercel/static"
        }
    ],
    "routes":[
        {
            "src": "/api/(.*)",
            "dest": "index.js"
        },
        {
            "src": "/dev/sqlite/viewer",
            "dest": "frontend/sqlite/index.html"
        },
        {
            "src": "/dev/sqlite/(.*)",
            "dest": "frontend/sqlite/$1"
        },
        {
            "src": "/(.*)",
            "dest": "client/build/$1" 
        },
        {
            "src": "/",
            "dest": "client/build/index.html"
        }
    ]
}