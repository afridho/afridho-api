{
    "version" : 2,
    "builds":[
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "client/build/**",
            "use": "@vercel/static"
        },
        {
            "src": "frontend/sqlite_viewer/**",
            "use": "@vercel/static"
        }
    ],
    "routes":[
        {
            "src": "/api/(.*)",
            "dest": "index.js"
        },
        {
            "src": "/(.*)",
            "dest": "client/build/$1" 
        },
        {
            "src": "/",
            "dest": "client/build/index.html"
        },
        {
            "src": "/sqlite-viewer/(.*)",
            "dest": "frontend/sqlite_viewer/$1" 
        },
        {
            "src": "/sqlite-viewer/",
            "dest": "frontend/sqlite_viewer/index.html"
        }
    ]
}