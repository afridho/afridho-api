{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "client/build/**",
            "use": "@vercel/static"
        },
        {
            "src": "frontend/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "index.js"
        },
        {
            "src": "/dev/(.*)",
            "dest": "frontend/$1"
        },
        {
            "src": "/dev/sqlite",
            "dest": "frontend/sqlite/index.html"
        },
        {
            "src": "/(.*)",
            "dest": "client/build/$1"
        },
        {
            "src": "/",
            "dest": "client/build/index.html"
        },
        {
            "src": "/public/(.*)",
            "dest": "public/$1"
        }
    ]
}
